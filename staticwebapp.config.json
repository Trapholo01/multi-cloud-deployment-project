{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"],
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/*.json", "/*.svg", "/*.png", "/*.jpg", "/*.gif"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".wasm": "application/wasm"
  }
}
